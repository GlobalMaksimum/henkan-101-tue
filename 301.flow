nodes:
  - name: SupplierDim
    type: scd
    config:
      subtype: spark
      conf: conf/f_301_supplierdim.json
      target: hive
      explain_only: false
      source_table: default.supplier
      change_tolerance: 10
      target_table: default.dim_supplier
    dependsOn:
      - LoadSupplierIntoHive
  - name: LoadSupplierIntoHive
    type: transfer
    config:
      subtype: spark
      conf: conf/f_301_loadsupplierintohive.json
      source: PGLab
      target: hive
      explain_only: false
      empty_source_check: true
      source_table: public.supplier
      target_table: default.supplier
    dependsOn: []
  - name: TransferToVertica
    type: transfer
    config:
      subtype: spark
      conf: conf/f_301_transfertovertica.json
      source: hive
      target: veon10
      source_table: default.dim_supplier
      explain_only: false
      empty_source_check: true
      target_table: public.dim_supplier
    dependsOn:
      - Truncate dimension table
  - name: Truncate dimension table
    type: transform
    config:
      target: veon10
      sql_file: sql/f_301_truncate dimension table.sql
      explain_only: false
    dependsOn:
      - SupplierDim
config: {}
