nodes:
  - name: PGtoHive
    type: transfer
    config:
      subtype: spark
      conf: conf/f_401_pgtohive.json
      source: PGLab
      target: hive
      explain_only: false
      empty_source_check: true
      source_table: public.lineitem
      target_table: default.lineitem
    dependsOn: []
  - name: HadoopAgg
    type: transform
    config:
      target: v360hive
      sql_file: sql/f_401_hadoopagg.sql
      explain_only: false
    dependsOn:
      - PGtoHive
config: {}
